    "serverless-offline": "^3.15.3",
    "serverless-plugin-typescript": "^1.0.0",
    "source-map-support": "^0.4.15",
    "ts-node": "^3.3.0",
    "typings": "^2.1.1"
  },
  "dependencies": {
    "@types/mustache": "^0.8.29",
    "@types/node": "^8.0.17",
